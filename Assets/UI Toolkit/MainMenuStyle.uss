.header {
    font-size: 50px;
    -unity-text-align: upper-center;
    -unity-font-style: bold;
    margin-top: 60px;
}

.button {
    font-size: 30px;
    width: 220px;
    height: 80px;
    align-items: stretch;
    align-self: center;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    border-bottom-left-radius: 10px;
    margin-top: 50px;
}

.logo {
    align-self: center;
    margin-top: 40px;
    font-size: 40px;
}
